{
  "name": "RealLifeRPG",
  "version": "2.3.9",
  "description": "RealLifeRPG Launcher",
  "homepage": "https://www.realliferpg.de",
  "author": "RealLifeRPG",
  "main": "main.js",
  "license": "GPL-3.0",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/A3ReallifeRPG/RealLifeRPGLauncher.git"
  },
  "bugs": {
    "url": "https://github.com/A3ReallifeRPG/RealLifeRPGLauncher/issues"
  },
  "scripts": {
    "start": "electron .",
    "pack": "build --target dir",
    "dist": "build",
    "test": "standard"
  },
  "standard": {
    "ignore": [
      "config.js"
    ]
  },
  "build": {
    "appId": "realliferpg.de",
    "mac.category": "launcher.application",
    "win": {
      "title": "RealLifeRPG Launcher",
      "iconUrl": "https://static.realliferpg.de/img/launcher/icon.ico",
      "remoteReleases": "https://static.realliferpg.de/remoteReleases/",
      "target": "squirrel"
    },
    "squirrelWindows": {
      "loadingGif": "build/install-spinner.gif",
      "setupIcon": "build/icon.ico"
    },
    "extraFiles": "README.md"
  },
  "devDependencies": {
    "devtron": "1.4.0",
    "electron": "^1.6.8",
    "electron-builder": "10.15.1",
    "standard": "*"
  },
  "contributors": [
    {
      "name": "Kaibu",
      "email": "notkaibu@gmail.com",
      "url": "https://github.com/Kaibu"
    },
    {
      "name": "vabene1111",
      "email": "vabene1234@googlemail.com",
      "url": "https://github.com/vabene1111"
    }
  ],
  "dependencies": {
    "alertify": "^0.3.0",
    "angular": "^1.6.4",
    "angular-tooltips": "^1.1.10",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.5.0",
    "electron-json-storage": "^2.1.0",
    "electron-squirrel-startup": "1.0.0",
    "font-awesome": "^4.7.0",
    "hasha": "^3.0.0",
    "humanize-duration": "^3.10.0",
    "icheck": "^1.0.2",
    "jquery": "^3.1.1",
    "jsonist": "^2.1.0",
    "leaflet": "^1.0.3",
    "marked": "^0.3.6",
    "mkpath": "1.0.0",
    "perfect-scrollbar": "^0.7.0",
    "ps-node": "^0.1.6",
    "request": "^2.81.0",
    "request-progress": "^3.0.0",
    "smoothie": "^1.27.0",
    "tether": "^1.4.0",
    "tether-shepherd": "^1.8.1",
    "unzip": "^0.1.11",
    "webtorrent": "^0.98.18",
    "winreg": "^1.2.4"
  }
}
