{
  "name": "RealLifeRPG",
  "version": "2.7.2",
  "description": "RealLifeRPG Launcher",
  "homepage": "https://www.realliferpg.de",
  "author": "RealLifeRPG",
  "main": "main.js",
  "license": "GPL-3.0",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/A3ReallifeRPG/RealLifeRPGLauncher.git"
  },
  "bugs": {
    "url": "https://github.com/A3ReallifeRPG/RealLifeRPGLauncher/issues"
  },
  "scripts": {
    "start": "electron .",
    "dist": "build && build -w portable && build -w appx",
    "portable": "build -w portable",
    "appx": "build -w appx",
    "test": "standard --fix",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "standard": {
    "globals": [
      "angular",
      "ipcRenderer",
      "$",
      "alertify",
      "helpers",
      "Chart",
      "path",
      "shell",
      "config",
      "app",
      "fs",
      "storage",
      "dialog",
      "iCheck",
      "Shepherd",
      "marked",
      "moment",
      "ping",
      "child",
      "unzip",
      "Winreg",
      "humanizeDuration",
      "L",
      "prettyBytes"
    ],
    "ignore": [
      "app/require.js"
    ]
  },
  "build": {
    "appId": "realliferpg.de",
    "productName": "RealLifeRPG",
    "copyright": "Copyright Â©2018 RealLifeRPG",
    "win": {
      "target": "squirrel"
    },
    "squirrelWindows": {
      "iconUrl": "https://static.realliferpg.de/img/launcher/icon.ico",
      "loadingGif": "build/install-spinner.gif",
      "remoteReleases": "https://static.realliferpg.de/remoteReleases/"
    },
    "appx": {
      "applicationId": "Kaibu.ReallifeRPG",
      "publisher": "CN=3E2F6B16-28C3-4276-919B-C80D5AE8D520",
      "backgroundColor": "#2b3e50",
      "displayName": "RealLifeRPG",
      "identityName": "13873Kaibu.ReallifeRPG",
      "publisherDisplayName": "Kaibu",
      "languages": [
        "de"
      ]
    },
    "portable": {
      "requestExecutionLevel": "user"
    },
    "extraFiles": "README.md"
  },
  "devDependencies": {
    "devtron": "1.4.0",
    "electron-builder": "19.42.0",
    "electron-builder-squirrel-windows": "19.42.0",
    "electron": "1.7.15",
    "standard": "*"
  },
  "contributors": [
    {
      "name": "Kaibu",
      "email": "notkaibu@gmail.com",
      "url": "https://github.com/Kaibu"
    },
    {
      "name": "vabene1111",
      "email": "vabene1234@googlemail.com",
      "url": "https://github.com/vabene1111"
    }
  ],
  "dependencies": {
    "alertify": "^0.3.0",
    "angular": "^1.6.7",
    "angular-tooltips": "^1.2.2",
    "async": "^2.6.0",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.7.1",
    "electron-json-storage": "^2.1.0",
    "electron-squirrel-startup": "1.0.0",
    "font-awesome": "^4.7.0",
    "hasha": "^3.0.0",
    "humanize-duration": "^3.10.0",
    "icheck": "^1.0.2",
    "jquery": "^3.1.1",
    "jsonist": "^2.1.0",
    "leaflet": "^1.2.0",
    "marked": "^0.3.6",
    "mkpath": "1.0.0",
    "moment": "^2.18.1",
    "perfect-scrollbar": "^0.7.0",
    "ping": "^0.2.2",
    "pretty-bytes": "^4.0.2",
    "ps-node": "^0.1.6",
    "recursive-readdir": "^2.2.1",
    "request": "^2.83.0",
    "request-progress": "^3.0.0",
    "tether": "^1.4.3",
    "tether-shepherd": "^1.8.1",
    "unzip": "^0.1.11",
    "webtorrent": "^0.98.20",
    "winreg": "^1.2.4"
  }
}
